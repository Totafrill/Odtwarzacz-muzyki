<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="195.0" prefWidth="658.0" style="-fx-background-color: #1d38cf;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Odtwarzacz.MainWindowController">
   <children>
      <Label fx:id="titleLabel" alignment="CENTER" layoutX="27.0" prefHeight="84.0" prefWidth="602.0" text="Odwarzacz muzyki" textFill="WHITE">
         <font>
            <Font size="53.0" />
         </font>
      </Label>
      <ProgressBar fx:id="progressBar" layoutY="92.0" prefHeight="18.0" prefWidth="646.0" progress="0.0" />
      <Button fx:id="playButton" layoutX="100.0" layoutY="123.0" mnemonicParsing="false" onAction="#playMusic" prefHeight="50.0" prefWidth="77.0" text="PLAY">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="pauseButton" layoutX="178.0" layoutY="123.0" mnemonicParsing="false" onAction="#pauseMusic" prefHeight="50.0" prefWidth="78.0" text="PAUSE">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="resetButton" layoutX="256.0" layoutY="123.0" mnemonicParsing="false" onAction="#resetMusic" prefHeight="50.0" prefWidth="78.0" text="RESET">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="perviousButton" layoutX="14.0" layoutY="123.0" mnemonicParsing="false" onAction="#previousMusic" prefHeight="50.0" prefWidth="86.0" text="PREVIOUS">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="nextButton" layoutX="334.0" layoutY="123.0" mnemonicParsing="false" onAction="#nextMusic" prefHeight="50.0" prefWidth="59.0" text="NEXT">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="listButton" layoutX="393.0" layoutY="123.0" mnemonicParsing="false" onAction="#addToPlaylist" prefHeight="50.0" prefWidth="106.0" text="ADD TO PLAYLIST">
         <font>
            <Font size="10.0" />
         </font>
      </Button>
      <Slider fx:id="volumeSlider" layoutX="506.0" layoutY="141.0" max="100.0" value="50.0" />
   </children>
</AnchorPane>
